<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="au.gov.nla.domain">

	<class name="BookOut" table="t_BookOut">

          
          <id name="outId" column="lId">
            <generator class="native"/>
          </id>
         <property
            name="bookId"
            column="bId"
            type="int"
          />
          <property
            name="userId"
            column="uid"
            type="int"

          />
          <property
            name="outTime"
            column="out_time"
            type="String"
            length="18"

          />
	</class>
	
</hibernate-mapping>
 
